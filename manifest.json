{
    "manifest_version": 2,
    "name": "AWS COLOR CHANGER",
    "description": "Change color of header & footer on AWS Management Console.",
    "version": "1.0",
    "permissions": ["storage"], 
    "options_page": "options.html", 
    "content_scripts": [
      {
        "matches": ["http://*.aws.amazon.com/*","https://*.aws.amazon.com/*"],
        "js": ["js/colorAWSFrame.js"]
      }
    ]
  }